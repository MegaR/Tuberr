{
    "include": ["server/app.ts"],
    "exclude": [],
    "compilerOptions": {
        "outDir": "./",
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "moduleResolution": "node"
    }
}